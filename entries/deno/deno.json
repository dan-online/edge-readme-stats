{
	"name": "@edge-readme-stats/deno",
	"version": "0.1.0",
	"exports": "./src/main.ts",
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx"
	},
	"imports": {
		"@edge-readme-stats/core": "../../core/src/index.tsx",
		"@edge-readme-stats/core/": "../../core/src/",
		"hono": "npm:hono@^4.11.5",
		"hono/jsx": "npm:hono@^4.11.5/jsx",
		"hono/jsx/jsx-runtime": "npm:hono@^4.11.5/jsx/jsx-runtime",
		"hono/jsx/jsx-dev-runtime": "npm:hono@^4.11.5/jsx/jsx-dev-runtime",
		"hono/jsx/streaming": "npm:hono@^4.11.5/jsx/streaming",
		"ky": "npm:ky@^1.14.3",
		"valibot": "npm:valibot@^1.2.0",
		"hono-openapi": "npm:hono-openapi@^1.2.0",
		"hono-openapi/": "npm:hono-openapi@^1.2.0/",
		"@hono/valibot-validator": "npm:@hono/valibot-validator@^0.6.1",
		"@valibot/to-json-schema": "npm:@valibot/to-json-schema@^1.5.0",
		"@hono/swagger-ui": "npm:@hono/swagger-ui@^0.5.3",
		"@hono/standard-validator": "npm:@hono/standard-validator@^0.2.2",
		"@standard-community/standard-json": "npm:@standard-community/standard-json@^0.3.5",
		"@standard-community/standard-openapi": "npm:@standard-community/standard-openapi@^0.2.9",
		"@standard-schema/spec": "npm:@standard-schema/spec@^1.1.0",
		"openapi-types": "npm:openapi-types@^12.1.3",
		"quansync": "npm:quansync@^1.0.0"
	},
	"tasks": {
		"dev": "deno run --allow-net --allow-env --allow-read --watch src/main.ts",
		"start": "deno run --allow-net --allow-env --allow-read src/main.ts",
		"sync-imports": "deno run --allow-read --allow-write scripts/sync-imports.ts && deno cache src/main.ts"
	}
}
